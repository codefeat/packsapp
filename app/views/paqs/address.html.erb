<div id="throbber" style="display:none; min-height:120px;"></div>
<div id="noty-holder"></div>
<div id="wrapper">
  <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
     <%= render 'layouts/navbartop' %>
     <% if user_signed_in? %>
       <%= render 'layouts/navbarright' %>
       <%= render 'layouts/navbarside' %>
     <% end %>
  </nav>

    <div id="page-wrapper">
        <div class="container-fluid">
            <!-- Page Heading -->
            <div class="row" id="main" >
                    <div class="row">
                    <h1>Your NextPakk Address</h1>
                        <div class="profile-header-container" style="margin-top: 10px;">   
                            <div class="profile-header-img">
                            <%= image_tag current_user.image, class: "img-responsive img-circle center-block" %>                            <!-- badge -->
                                <div class="rank-label-container">
                                    <span class="label label-default rank-label"><%= current_user.first_name %></span>
                                </div>
                            </div>
                        </div> 
                    </div>
                <div class="step1">
                <%= render 'layouts/tabnav' %>
                      <br /><br /><br />
                      <div class="cust-mob-body-text">
                      <br /><br /><h2>Your NextPakk Address</h2><br />
                      <p class="mobile-para" style="text-align: left !important;">
                      <%= "#{current_user.first_name.capitalize} #{current_user.last_name.capitalize}" %><br />
                      3601 25th Ave S.<br />
                      NextPakk #<%= current_user.packs_num %><br />
                      Minneapolis, MN 555406	
                      </p>
                      </div>
                </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </div>
    <!-- /#page-wrapper -->
</div><!-- /#wrapper -->

